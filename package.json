{
  "name": "kanji-pre-reader",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@testing-library/react": "^11.2.2",
    "html2canvas": "^1.0.0-rc.7",
    "jspdf": "^2.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-pdf": "^5.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "styled-components": "^5.2.1",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && cp _redirects build/_redirects",
